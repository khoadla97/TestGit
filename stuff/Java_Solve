		int result = 0;
		int count = 0;
		int n = number;
		if (number < 10) {
			result = number;
		}
		else {
		    int dem = 0;
		    while(n >= 10){
		        n /= 10;
		        dem ++;
		    }
		    count = dem + 1;
		    for (int i = 1; i <= count; i++) {
		    	if (i < count) {
		    		result += i*(Math.pow(10, i) - Math.pow(10, i - 1));
		    		System.out.println(result);
		    	}
		    	else
		    		result += i*(number - (Math.pow(10,i -1) - 1));
		    }
		}
